<p id="notice"><%= notice %></p>

<h1>Listing Help Session Records</h1>

<table>
  <thead>
    <tr>
      <th>Student</th>
      <th>User</th>
      <th>Date</th>
      <th>Learning plan</th>
      <th>Comments</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @help_session_records.each do |help_session_record| %>
      <tr>
        <td><%= help_session_record.student_id %></td>
        <td><%= help_session_record.user_id %></td>
        <td><%= help_session_record.date %></td>
        <td><%= help_session_record.learning_plan_id %></td>
        <td><%= help_session_record.comments %></td>
        <td><%= link_to 'Show', help_session_record %></td>
        <td><%= link_to 'Edit', edit_help_session_record_path(help_session_record) %></td>
        <td><%= link_to 'Destroy', help_session_record, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

