<div class="card card-default">
  <div class="card-header">
    <h3 style="display:inline;" class="card-title">HW Help Sessions
  </div>
  <div class="card-block"
  style="padding:20px 15px 0 15px;max-height:100px;overflow:scroll;">
    <!-- displays note form and notes for student -->
    <% @student.help_session_records.each do |help_session| %>
      <%= render 'help_session_card', help_session: help_session %>
    <% end %>
  </div>
</div>
