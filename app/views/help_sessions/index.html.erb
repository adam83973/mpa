<p id="notice"><%= notice %></p>

<h1>Listing Help Sessions</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>User</th>
      <th>Learning plan</th>
      <th>Comments</th>
      <th>Parent feedback</th>
      <th>Student</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @help_sessions.each do |help_session| %>
      <tr>
        <td><%= help_session.date %></td>
        <td><%= help_session.user_id %></td>
        <td><%= help_session.learning_plan_id %></td>
        <td><%= help_session.comments %></td>
        <td><%= help_session.parent_feedback %></td>
        <td><%= help_session.student_id %></td>
        <td><%= link_to 'Show', help_session %></td>
        <td><%= link_to 'Edit', edit_help_session_path(help_session) %></td>
        <td><%= link_to 'Destroy', help_session, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Help session', new_help_session_path %>
