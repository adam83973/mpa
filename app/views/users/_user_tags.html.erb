<div class="panel panel-default">
  <div class="panel-heading">
    <h3 style="display:inline;" class="panel-title">Tags</h3>
  </div>
  <div class="panel-body">
    <div class="tags">
      <table class='table table-striped table-bordered'>
        <tr>
          <th>Tag Name</th>
          <th>Tag Id</th>
        </tr>
        <% @user.infusionsoft_tags.each do |tag| %>
          <tr>
            <td>
              <%= tag['GroupId'] %>
            </td>
            <td>
              <%= tag['ContactGroup'] %>
            </td>
          </tr>
        <% end %>
      </table>
    </div>
  </div>
</div>
