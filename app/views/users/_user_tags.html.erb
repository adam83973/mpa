<div class="panel panel-default">
  <div class="panel-heading">
    <h3 style="display:inline;" class="panel-title">Tags</h3>
  </div>
  <div class="panel-body">
    <div class="tags">
      <ul>
        <% @user.infusionsoft_tags.each do |tag| %>
          <li> <%= tag['ContactGroup'] %>(<%= tag['GroupId'] %>) </li>
        <% end %>
      </ul>
    </div>
  </div>
</div>
