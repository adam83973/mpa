<div class="card card-default">
  <div class="card-header">
    <h3 style="display:inline;" class="card-title">Tags</h3>
  </div>
  <div class="card-block">
    <div class="tags">
      <table class='table table-striped table-bordered header-fixed'>
        <thead>
          <tr>
            <th>Tag Id</th>
            <th>Tag Name</th>
          </tr>
        </thead>
        <tbody>
          <% @user.infusionsoft_tags.each do |tag| %>
            <tr>
              <td><%= tag['GroupId'] %></td>
              <td><%= tag['ContactGroup'] %></td>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
