<% unless @student || @lead %>
  <% @user.notes.order("created_at DESC").each do |note| %>
    <li>
      <div class="note">
        <p>
          <%= note.content %>
        </p>
        <%= note.user.full_name%> (<%= note.created_at.strftime("%B %d, %Y")%>)
      </div>
    </li>
  <% end %>
<% end %>
