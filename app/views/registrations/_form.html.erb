<%= simple_form_for(@registration) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :start_date %>
    <%= f.input :end_date %>
    <%= f.input :hold_date %>
    <%= f.input :trial_date %>
    <%= f.input :attended_first_class %>
    <%= f.input :attended_trial %>
    <%= f.input :student_id %>
    <%= f.input :offering_id %>
    <%= f.input :admin_id %>
    <%= f.input :status %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
