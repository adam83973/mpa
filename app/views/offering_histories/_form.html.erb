<%= simple_form_for(@offering_history) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :offering_id %>
    <%= f.input :teacher_id %>
    <%= f.input :assistant_id %>
    <%= f.input :enrollment %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
